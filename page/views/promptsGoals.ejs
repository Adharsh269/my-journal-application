<%- include('partials/header.ejs') %>
    <div class="container">
        <div class="d-flex flex-column">
            <div class="mt-5">
                <h1><%- locals.prompts ? "Prompts" : locals.goals ? "Life Goals" : "Nothing" %></h1>
            </div>
            <% if(locals.prompts) { %>
                <% prompts.prompts.forEach((prompt) => { %>
                    <h3 style="color: #44423D;" class="mt-2"><%- prompt ? `<li contentEditable="true" class="ps-4">${prompt}</li>` : "No prompts" %></h3>
                <% }) %>
                <div class="mt-5"   >
                    <button class="btn btn-primary">Add Prompt</button>
                </div>
            <% } else if( locals.goals) { %>
                <% goals.lifegoals.forEach((lg) => { %>
                    <h3 style="color: #44423D;" class="mt-2"><%- lg ? `<li contentEditable="true" class="ps-4">${lg}</li>` : "No prompts" %></h3>
                <% }) %>
                <div class="mt-5"   >
                    <button class="btn btn-primary">Add Goal</button>
                </div>
            <% } %>
        </div>
    </div>
<%- include('partials/footer.ejs') %>